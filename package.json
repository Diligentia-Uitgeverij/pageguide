{
  "name": "pageguide",
  "version": "2.0.7",
  "description": "A simple library to light page-elements up, even in complex z-index rich applications.",
  "license": "GPL-2.0",
  "main": "./dist/pageguide.js",
  "typings": "./dist/types/PageGuide.d.ts",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --open",
    "docs": "typedoc --readme none --mode file --theme markdown --out ./doc/ ./src/PageGuide.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Diligentia-Uitgeverij/pageguide.git"
  },
  "keywords": [
    "pageguide",
    "support",
    "help",
    "tutorial"
  ],
  "directories": {
    "doc": "./doc",
    "lib": "./src",
    "example": "./src/example"
  },
  "author": "Peter Delvaux <peter.delvaux@diligentia-uitgeverij.be>",
  "dependencies": {
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.3.0",
    "typedoc": "^0.11.1",
    "typedoc-plugin-markdown": "^1.1.12",
    "typescript": "^2.8.4",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  }
}
